[project]
	description = Route resolution library for things that need something like RESTful app route processing
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group afiniate
	owner = group user/Eric Merritt (ericbmerritt)
[access "refs/for/refs/*"]
	read = group afiniate
	read = group user/Eric Merritt (ericbmerritt)
	push = group afiniate
	push = group user/Eric Merritt (ericbmerritt)
	label-Code-Review = -2..+2 group afiniate
