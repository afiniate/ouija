	label-Verified = -1..+1 group afiniate
	label-Verified = -1..+1 group user/Eric Merritt (ericbmerritt)
	rebase = group afiniate
	rebase = group user/Eric Merritt (ericbmerritt)
	removeReviewer = group afiniate
	removeReviewer = group user/Eric Merritt (ericbmerritt)
	submit = group afiniate
	submit = group user/Eric Merritt (ericbmerritt)
[access "refs/heads/*"]
	read = group afiniate
	read = group user/Eric Merritt (ericbmerritt)
	create = group afiniate
	create = group user/Eric Merritt (ericbmerritt)
	push = group afiniate
	push = +force group user/Eric Merritt (ericbmerritt)
	pushMerge = group afiniate
	pushMerge = group user/Eric Merritt (ericbmerritt)
[access "refs/tags/*"]
	push = group afiniate
	push = +force group user/Eric Merritt (ericbmerritt)
